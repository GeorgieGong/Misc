\mode<presentation>

% this is the main color entry for the theme


\definecolor{beamer@blendedblue}{rgb}{0.5,0.5,0.3} % changed this                                                                                                                                                                
\definecolor{beamer@kaustblue}{rgb}{0.180, 0.552, 0.588}
\definecolor{beamer@kaustorange}{rgb}{0.906, 0.443, 0.129}

\definecolor{beamer@kaustorangel}{rgb}{0.952,  0.465,  0.136}
\definecolor{beamer@kaustorangell}{rgb}{1.0,  0.489,  0.142}



\definecolor{beamer@kaustoranged}{rgb}{ 0.815,  0.399,  0.116}
\definecolor{beamer@kaustorangedd}{rgb}{0.734,  0.359,  0.105}
\definecolor{beamer@kaustorangeddd}{rgb}{0.660,  0.323,  0.094}

\definecolor{beamer@kaustwhite}{rgb}{1.0, 1.0, 1.0}
\definecolor{beamer@kaustgreen}{rgb}{0.459, 0.780, 0.098}
\definecolor{beamer@kaustyellow}{rgb}{0.965, 0.718, 0.122}
\definecolor{beamer@kaustblack}{rgb}{0.0, 0.0, 0.0}
\definecolor{beamer@kaustgrey}{rgb}{0.502, 0.463, 0.435}


\setbeamercolor{normal text}{fg=beamer@kaustblack,bg=white}
\setbeamercolor{alerted text}{fg=beamer@kaustorangedd}
\setbeamercolor{example text}{fg=beamer@kaustoranged}

%\setbeamercolor{structure}{fg=beamer@blendedblue}                                                                                                                                                                               

%\setbeamercolor{background canvas}{parent=normal text}                                                                                                                                                                          
%\setbeamercolor{background}{parent=background canvas}                                                                                                                                                                           

%\definecolor{wireblue}{RGB}{190,30,60}
\setbeamercolor*{structure}{fg=beamer@kaustorange,bg=beamer@kaustwhite}

% the following are almost all default
\setbeamercolor{normal text}{fg=beamer@kaustgrey,bg=beamer@kaustwhite}
\setbeamercolor{alerted text}{fg=beamer@kaustorange}
\setbeamercolor{background canvas}{parent=normal text}
\setbeamercolor{background}{parent=background canvas}

\setbeamercolor{block title}{fg=white,bg=beamer@kaustoranged}
\setbeamercolor{block title alerted}{use=alerted text,fg=white,bg=beamer@kaustorangeddd} % first was white
\setbeamercolor{block title example}{use=example text,fg=white,bg=beamer@kaustorangeddd}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!15!bg}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!15!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!15!bg}

\setbeamercolor*{palette primary}{fg=beamer@kaustwhite,bg=beamer@kaustorange}
\setbeamercolor*{palette secondary}{fg=beamer@kaustwhite,bg=beamer@kaustorangel}
\setbeamercolor*{palette tertiary}{fg=beamer@kaustwhite,bg=beamer@kaustorangell}
\setbeamercolor*{palette quaternary}{fg=beamer@kaustwhite,bg=beamer@kaustorangell}

\setbeamercolor*{sidebar}{use=structure,bg=structure.fg}

\setbeamercolor*{palette sidebar primary}{bg=beamer@kaustwhite,fg=beamer@kaustorangell}
\setbeamercolor*{palette sidebar secondary}{bg=beamer@kaustgreen,fg=beamer@kaustwhite}
\setbeamercolor*{palette sidebar tertiary}{fg=beamer@kaustorangell}
\setbeamercolor*{palette sidebar quaternary}{fg=beamer@kaustwhite}

\setbeamercolor*{titlelike}{parent=palette primary}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

% some custom colors
\setbeamercolor*{Title bar}{bg=beamer@kaustorange,fg=beamer@kaustwhite}
\setbeamercolor*{frametitle}{parent=Title bar}
\setbeamercolor{author in header}{fg=beamer@kaustblack}
%\setbeamercolor{section in head/foot}{use=structure,bg=structure.fg!10,fg=beamer@kaustblack}
\setbeamercolor{subsection in head/foot}{fg=beamer@kaustblack}


\newcommand{\ypebble}[0]{\includegraphics[height=1.3 ex]{./k_yellowpebble.pdf}}

\newcommand{\bpebble}[0]{\includegraphics[height=1.3 ex]{./k_bluepebble.pdf}}
\newcommand{\bpebblelarge}[0]{\includegraphics[height=1.6 ex]{./k_bluepebble.pdf}}
\newcommand{\bpebblexlarge}[0]{\includegraphics[height=1.9 ex]{./k_bluepebble.pdf}}

\newcommand{\opebble}[0]{\includegraphics[height=1.3 ex]{./k_orangepebble.pdf}}
\newcommand{\opebblesmall}[0]{\includegraphics[height=1.1 ex]{./k_orangepebble.pdf}}
\newcommand{\opebblexsmall}[0]{\includegraphics[height=0.9 ex]{./k_orangepebble.pdf}}

\newcommand{\gpebble}[0]{\includegraphics[height=1.3 ex]{./k_greenpebble.pdf}}

\setbeamertemplate{itemize items}{\gpebble}

\setbeamertemplate{enumerate items}{\bpebblexlarge \hspace*{-2.3 ex} \color{beamer@kaustwhite} \theenumi }

\setbeamertemplate{section in toc}{\hspace*{1em} \opebble \hspace*{0.2 em}  \inserttocsection}
\setbeamertemplate{subsection in toc}{\hspace*{2em} \opebblesmall \hspace*{0.2 em} \inserttocsubsection}
\setbeamertemplate{subsubsection in toc}{\hspace*{3em} \opebblexsmall \hspace*{0.2 em} \inserttocsubsubsection}

\mode<all>
